[% global.ps.loaded.lines = 1 -%]
/line-light  { 
    0.25 setlinewidth 
    [] 0 setdash 
} bind def
/line-normal { 
    0.5 setlinewidth 
    [] 0 setdash 
} bind def
/line-heavy  { 
    0.75 setlinewidth 
    [] 0 setdash 
} bind def
/line-thick {
  1.5 setlinewidth
  [] 0 setdash
} def
/line-dotted { 
    0.5 setlinewidth 
    [3 5 1 5] 0 setdash 
} def
/line-dashed { 
    0.5  setlinewidth 
    [5 3 2 3] 0 setdash 
} def
